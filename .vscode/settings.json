{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "i18n-ally.pathMatcher": "{locale}.json",
  "i18n-ally.enabledParsers": ["json"],
  "i18n-ally.localesPaths": "frontend/src/locales",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.enabledFrameworks": ["vue"],
  "i18n-ally.editor.preferEditor": true,
  "i18n-ally.displayLanguage": "en",
  "i18n-ally.sourceLanguage": "en",
  "i18n-ally.keysInUse": ["$vuetify.*", "errors.*", "prop.toolkitdata.*"],
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "search.exclude": {
    "**/dist": true
  },
  "files.exclude": {
    "**/.project": true
  },
  "vue.autoInsert.dotValue": true,
  "editor.suggest.insertMode": "replace",
  "editor.snippetSuggestions": "none",
  "editor.suggest.localityBonus": true,
  "editor.suggest.selectionMode": "always",
  "editor.tabSize": 2,
  "html.autoCreateQuotes": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "javascript.suggest.names": true,
  "autoRestartTsESLint.includeFilesForTypescript": "**/node_modules/.prisma/client/index.js",
  "explorer.autoRevealExclude": {
    "**/node_modules": false
  },
  "files.watcherExclude": {
    "**/dist/**": true,
    "**/node_modules/*/**": false,
    "**/node_modules/!(.prisma)/**": true
  },
  "autoRestartTsESLint.monitorFilesForTypescript": true,
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  }
}
